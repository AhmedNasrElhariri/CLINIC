<!doctype html >
<html>

<head>
  <title>Clinic-R Report</title>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
</head>
<style>
  table {
    border-collapse: collapse;
    width: 100%;
    margin: 0px 2px 50px 2px;
  }
  
  th, td {
    text-align: left;
    padding: 8px;
  }
  .heading{
    font-size: 15px;
    margin: 5px;
  }
  
  #accounting{
    text-align: center;
    margin: 5px 0px 10px 0x;
  }
  tr:nth-child(even) {background-color: #f2f2f2;}
  </style>

<body>
  <div class="test">
    <h2 id="accounting">Insurrance Report</h2>
    <p class="heading">from <%= formatDateStandard(updatedDateFrom) %> to <%= formatDateStandard(updatedDateTo) %></p>
    <p class="heading">Total Revenues = <%= totalRevenues %></p>
    <hr></hr>
   
      <table>
        <tr>
          <th>Name</th><th>Amount</th><th>Date</th>
        </tr>
    
        <% for (var i = 0; i < revenues.length; /* I save the data in a variable 'quotation', I don't know how you named your variable  */  i++) { %>
            <tr>
              <td><%= revenues[i].name %></td>
              <td><%= revenues[i].amount %></td>
              <td><%= formatDateStandard(revenues[i].date) %></td>
            </tr>    
        <% } %>
    </table>
    
  </div>
</body>

</html>